<!DOCTYPE html>
<html>
<head>
    <title>Degree Requirements</title>
    <%include partials/head%>
</head>
<body>
<% include partials/menu %>
<h1>Your degree requirements:</h1>
<table class="table">
    <thead>
    <h3>First Degree: <%= degree1%></h3>
    <tr>
        <th scope="col">Course Code</th>
        <th scope="col">Type</th>
    </tr>
    </thead>
    <tbody>
    <% for(var i=0; i<data1.length; i++) { %>
        <tr>
            <td><%= data1[i].required_cid%></td>
            <td><%= data1[i].type%></td>
        </tr>
    <% } %>
    </tbody>
</table>
<br>
<table class="table">
    <thead>
    <h3>Second Degree: <%= degree2%></h3>
    <tr>
        <th scope="col">Course Code</th>
        <th scope="col">Type</th>
    </tr>
    </thead>
    <tbody>
    <% for(var i=0; i<data2.length; i++) { %>
        <tr>
            <td><%= data2[i].required_cid%></td>
            <td><%= data2[i].type%></td>
        </tr>
    <% } %>
    </tbody>
</table>
</body>
</html>
